# Docker Configuration
DOCKER_TARGET=dev
DOCKER_COMMAND=sh -c "pnpm install && pnpm dev"
VOLUME_MOUNT=.:/app

# Server Configuration
NODE_ENV=development
PORT=3000
HOST=0.0.0.0

# Database Configuration (for docker-compose)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=manila
POSTGRES_PORT=5432

# CORS Configuration
CORS_ORIGIN=http://localhost:3001,http://localhost:3002,http://localhost:5173,http://localhost:8081

# Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_TIME_WINDOW=15m

# Clerk Configuration
CLERK_PUBLISHABLE_KEY=your-clerk-publishable-key
CLERK_SECRET_KEY=your-clerk-secret-key
